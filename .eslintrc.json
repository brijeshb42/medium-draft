{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": [0],
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
    "new-cap": [0],
    "react/require-extension": "off",
    "max-len": [0]
  },
  "globals": {
    "describe": true,
    "it": true,
    "expect": true,
    "window": true,
    "__PROD__": true
  }
}
